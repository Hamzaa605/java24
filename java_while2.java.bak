import java.util.*;
class java_while2
{
	 
	 public static void main(String args[])
	 {
		Scanner sc=new Scanner(System.in);
		int n=0;
		
		
			while(n<0)
			{
				n=sc.nextInt();
			}
		
		int digitCount=0;
		
		while(n!=0)
		{
			
			n=n/10;
			digitCount++;
			
		}
		 System.out.println(digitCount);
		 if(digitCount>4)
		 {
			System.out.println("number is large"); 
		 }
		 else
		 {
			 System.out.println("number is not large"); 
		 }
		
	 }
	
}