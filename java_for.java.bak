import java.util.*;
class java_for
{
	public static void main(String args[])
	{
		/*inatializatin;condition;inc/dec*/
	System.out.println("increment");	
		for(int i=1; i<=5;i++)
		{
		 System.out.println(i);	
		}
	System.out.println("decrement");	
		for(int i=5; i>=1;i--)
		{
		 System.out.println(i);	
		}
	}
	
}